#!/bin/bash
my_dir="$(dirname "$0")"
. "$my_dir/common"

log-relation "ScaleIO client joining"

if [ $(is-leader) == 'True' ] ; then
    relation-set password=$(config-get password)
    relation-set port=$(config-get port)
    relation-set ip=$(get-ip)
    relation-set sdcs_ready=$(leader-get sdcs_ready)
fi
